<!--pages/main/main.wxml-->
<map
  style="width: 100%; height: 300px;"
  latitude="{{latitude}}"
  longitude="{{longitude}}"
  markers='{{markers}}'>
</map>
<view>
  <button bindtap='getLocation'>GetLocation</button>
  <button bindtap='putSomething'>PutSomething</button>
  <button bindtap='lookAround'>LookAround</button>
</view>
